{
    "version": 2,
    "builds": [
      {
        "src": "model.py",
        "use": "@vercel/python",
        "config": {
          "runtime": "python3.9"
        }
      }
    ],
    "functions": {
      "api/index.py": {
        "runtime": "@vercel/python"
      }
    },
    "routes": [
      { "handle": "filesystem" },
      { "src": "/(.*)", "dest": "/" }
    ]
  }
  