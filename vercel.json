{
    "version": 2,
    "builds": [
      {
        "src": "model.py",
        "use": "@vercel/python"
      }
    ],
    "build": {
      "env": {
        "PYTHON_VERSION": "3.9"
      }
    },
    "functions": {
      "source": "functions"
    },
    "routes": [
      { "handle": "filesystem" },
      { "src": "/(.*)", "dest": "/" }
    ]
  }
  